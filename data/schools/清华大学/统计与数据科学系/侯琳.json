'none')) {\n                $('.search-field').css('display', 'block')\n                $('.screen-reader-text').css('display', 'none')\n                $('.search-submit').css('display', 'block')\n\n            }\n            $('.search-field').css('display', 'block')\n        })\n     \n    </script>\n    <!-- 菜单点击添加active -->\n    <script>\n\n    </script>\n\n    <!-- inner-nav-list 右侧侧边栏 -->\n    <script>\n        jQuery(document).ready(function($) {\n\n            // Handle the click event of the div\n            $('.item').click(function() {\n                // Remove the \"active\" class from all divs\n                $('.item').removeClass('active');\n\n                // Add the \"active\" class to the clicked div\n                $(this).addClass('active');\n\n                // Save the ID of the clicked post to LocalStorage\n                localStorage.setItem('lastActivePostId', $(this).data('id'));\n            });\n\n            // Check if there is a saved ID for the last active post\n            var lastActivePostId = localStorage.getItem('lastActivePostId');\n            if (lastActivePostId) {\n                // Find the corresponding div and add the \"active\" class to it\n                $('.item[data-id=\"' + lastActivePostId + '\"]').addClass('active');\n\n            }\n\n            // 先给默认的第一个元素添加上熟悉，再判断其他元素是否有属性，若有则去掉第一个\n            $('.inner-nav-list .item:first-child').addClass('active');\n            $('.inner-nav-list').children(\".item\").each((i, e) => {\n                // console.log(i, e.className)\n                if (i != 0 && e.className.includes('active')) {\n                    $('.inner-nav-list .item:first-child').attr('class', 'item')\n                }\n            });\n\n        });\n\n     \n        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {\n            const links = document.querySelectorAll('#menu-menu-1 li a');\n\n            links.forEach(link => {\n        